<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>chiffre 5</title>
</head>
<body>

<script>
  document.write("<h2>Table de multiplication du chiffre 5</h2>");

  // Utilisation d'une boucle for pour générer la table de multiplication
  document.write("<ul>");
  for (let i = 1; i <= 10; i++) {
    //La boucle for est utilisée pour générer la table de multiplication du chiffre 5 de 1 à 10.
    let resultat = 5 * i;
    //À chaque itération de la boucle, le résultat de la multiplication (5 * i) est calculé et affiché dans un élément de liste (<li>).
    document.write("<li>5 * " + i + " = " + resultat + "</li>");
  }
  document.write("</ul>");
</script>

</body>
</html>
